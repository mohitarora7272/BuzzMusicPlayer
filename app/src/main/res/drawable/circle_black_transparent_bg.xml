<?xml version="1.0" encoding="utf-8"?><!--btn_capture.xml-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="32dp"
                android:color="#00000000" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <solid android:color="#40000000" />
            <stroke
                android:width="32dp"
                android:color="#00000000" />
        </shape>
    </item>
</layer-list>